{
	"name": "wrep/notificato",
	"type": "library",
	"description": "Send push notifications from PHP",
	"homepage": "https://github.com/wrep/notificato",
	"keywords": ["push", "pushnotifications", "apns", "ios"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mathijs Kadijk",
			"email": "mathijs@wrep.nl"
		},
		{
			"name": "Rick Pastoor",
			"email": "rick@wrep.nl"
		}
	],
	"support": {
		"issues": "https://github.com/wrep/notificato/issues"
	},
	"require": {
		"php": ">=5.3.3",
		"ext-spl": "*",
		"ext-sockets": "*",
		"ext-openssl": "*",
		"lib-openssl": "*",
		"psr/log": "1.0.*"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"sami/sami": "dev-master"
	},
	"suggest": {
		"wrep/notificato-symfony": "Integrate notificato into Symfony2"
	},
	"replace": {
		"wrep/notificare": "*"
	},
	"autoload": {
		"psr-0": { "Wrep\\Notificato\\": "src/" }
	}
}
